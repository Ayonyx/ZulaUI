<Font name="COMBAT_FEEDBACK" face="hyborian2" size="36" weight="Medium" italic="No" antialias="Full"/>
<Font name="HEADLINE" face="hyborian2" size="20" weight="Medium" italic="No" antialias="Full"/>
<Font name="LARGE_SC" face="Arial" size="14" weight="Normal" italic="No" antialias="None"/>

<Font name="3D_TEXT" face="Arial" size="30" weight="Medium" italic="No" antialias="Full"/>
<Font name="3D_TEXT_BOLD" face="Arial" size="30" weight="Bold" italic="No" antialias="Full"/>
<Font name="3D_TEXT_SMALL" face="Arial" size="25" weight="Medium" italic="No" antialias="Full"/>
